<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

<!--    <context:component-scan base-package="com.example.spring.learn.demo"/>-->

    <bean id="tiger" class="com.example.spring.learn.demo.entity.Tiger">
        <property name="name" value="喵喵"/>
    </bean>

    <bean id="monkey" class="com.example.spring.learn.demo.entity.Monkey">
        <property name="name" value="小孙"/>
    </bean>

    <bean id="animalFactory" class="com.example.spring.learn.demo.entity.AnimalFactory">
    </bean>

    <bean id="zoo" class="com.example.spring.learn.demo.entity.Zoo">
    </bean>

</beans>